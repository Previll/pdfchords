<?xml version="1.0"?>
<SyntaxDefinition name="Chord Pro Highlighting" xmlns="http://icsharpcode.net/sharpdevelop/syntaxdefinition/2008">
	<Color name="Command" foreground="Green" />
  <Color name="Chord" foreground="Red" />
	
	<RuleSet>

		<Span color="Command">
			<Begin>{</Begin>
			<End>}</End>
		</Span>

    <Span color="Chord">
      <Begin>\[</Begin>
      <End>\]</End>
    </Span>

	</RuleSet>
  
</SyntaxDefinition>